public class Rectangle{

 double length,breadth;
 double area;

 void Rectangle(double a,double b)
  {
    length=a;
    breadth=b;
  }

 void area()
  {
     area=length*breadth;
     System.out.println("the length of rectangle is:"+length+"\n the breadth of the rectangle is:"+breadth+"\n the area of the rectangle is:"+area);
  }

  void get()
   {
      System.out.println("the length of rectangle is:"+length+"\n the breadth of the rectangle is:"+breadth);

    }

  void set()
    {
       length=2*breadth;
       breadth=4*length;
    }
 
}
